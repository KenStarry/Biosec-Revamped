<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Biosec" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryVariant">@color/pink</item>
        <item name="colorOnPrimary">@color/black_light</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/pink</item>
        <item name="colorSecondaryVariant">@color/blue</item>
        <item name="colorOnSecondary">@color/black</item>
        <!--Text color-->
        <item name="textBlack900">@color/text_white_900</item>
        <item name="textBlack700">@color/text_white_700</item>
        <item name="textBlack500">@color/text_white_500</item>
        <item name="textBlack300">@color/text_white_300</item>
        <item name="textBlack100">@color/text_white_100</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorOnPrimary</item>
        <item name="android:navigationBarColor">?attr/colorOnPrimary</item>
        <!-- Customize your theme here. -->
    </style>
</resources>